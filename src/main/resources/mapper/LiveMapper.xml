<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hp.dao.LiveDao">

    <insert id="createRoom" parameterType="Live">
         insert into live(username,title,image,name,crossfire,roomNumber) values(#{username},#{title},#{image},#{name},#{crossfire},#{roomNumber})
    </insert>

    <select id="selectRoom" resultType="Live" parameterType="String">
        select title,username,image,name,crossfire,roomNumber from live where username = #{username}
    </select>



</mapper>
