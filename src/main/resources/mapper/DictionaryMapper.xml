<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hp.dao.DictionaryDao">


    <select id="getAllDic" resultType="com.hp.pojo.DictionaryDto">
        select * from dictionary
    </select>



    <select id="getDicByType" resultType="com.hp.pojo.DictionaryDto">
        select * from dictionary where serial_number = #{serialNumber} and parent_id is not null
    </select>

</mapper>
